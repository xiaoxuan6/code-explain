{
    "name": "james.xue/code-explain",
    "description": "What does it mean to explain the code（解释代码是什么意思）",
    "keywords": [
        "code",
        "analyze",
        "explain",
        "code-analyze",
        "code-explain"
    ],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/xiaoxuan6/code-explain/issues",
        "source": "https://github.com/xiaoxuan6/code-explain"
    },
    "authors": [
        {
            "name": "xiaoxuan6",
            "email": "1527736751@qq.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-sodium": "*",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^10.48",
        "laminas/laminas-text": "^2.0",
        "laravel-zero/framework": "^10.2",
        "laravel-zero/phar-updater": "^1.3",
        "laravel/prompts": "^0.1.17",
        "titasgailius/terminal": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.52",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "builds/code-explain"
    ],
    "scripts": {
        "pint": "vendor/bin/php-cs-fixer fix",
        "rector": "vendor/bin/rector process --no-diffs --ansi",
        "fix": [
            "@rector",
            "@pint"
        ]
    }
}
